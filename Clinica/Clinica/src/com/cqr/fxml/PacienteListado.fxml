<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1024.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.cqr.controller.PacienteListadoController">
  <children>
    <Pane fx:id="headertitle" layoutX="0.0" layoutY="0.0" prefHeight="40.0" prefWidth="1024.0" />
    <Label layoutX="14.0" layoutY="14.0" text="LISTADO DE PACIENTES" textFill="WHITE" />
    <TableView fx:id="tblPaciente" layoutX="14.0" layoutY="119.0" prefHeight="533.0" prefWidth="996.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="96.0" text="Nro. Historia" fx:id="clnNrohistoria" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="155.0" text="Nombre" fx:id="clnNombre" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="140.0" text="Apellido" fx:id="clnApellido" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="112.0" text="Direccion" fx:id="clnDireccion" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="164.0" text="Correo" fx:id="clnCorreo" />
        <TableColumn prefWidth="75.0" text="Celular" fx:id="clnCelular" />
        <TableColumn prefWidth="75.0" text="Tel. Fijo" fx:id="clnFijo" />
        <TableColumn prefWidth="75.0" text="Fec. Nacimiento" fx:id="clnFecNac" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="48.0" text="Edad" fx:id="clnEdad" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="43.0" text="Peso" fx:id="clnPeso" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="45.0" text="Talla" fx:id="clnTalla" />
      </columns>
    </TableView>
    <Button fx:id="btnBackToPaciente" layoutX="14.0" layoutY="665.0" mnemonicParsing="false" onAction="#backToPaciente" text="Regresar" />
    <Button fx:id="btnExportarExcel" layoutX="866.0" layoutY="665.0" mnemonicParsing="false" onAction="#exportarExcel" text="EXPORTAR A EXCEL" />
    <TextField fx:id="txtNrohistoriaApellido" layoutX="326.0" layoutY="63.0" prefHeight="36.0" prefWidth="684.0" />
    <Label layoutX="14.0" layoutY="73.0" text="Ingresa Apellido o Nro de Historia del paciente:" />
    <Button fx:id="btnEliminarPacSeleccionado" layoutX="593.0" layoutY="665.0" mnemonicParsing="false" onAction="#eliminarPacSeleccionado" text="ELIMINAR PACIENTE SELECCIONADO" />
  </children>
  <stylesheets>
    <URL value="@/com/cqr/style/style.css" />
  </stylesheets>
</AnchorPane>
